base silent = ( comments | import_any )*

ignore:[ ][\t]

import_any
	import_payment_type
	import_event
	import_item
	import_image
	import_trait
	import_aspect
	import_affinity
	import_condition
	import_activity | import_quest
	import_skill | import_attack | import_support
	import_map
	import_association | import_reason
	import_colour | import_animation_type | import_image | import_drawable | import_layer

comments Braced # # = [.*]
NAME = [[a-zA-Z_][a-zA-Z0-9_]*]
FLOAT = [[-]?\d+\.\d+]
INTEGER = [[-]?\d+]
FILE_NAME = [[^\.]+]
TITLE = [[^\n]+]
DESCRIPTION Braced ~ ~ = [.+]
ORDINAL = [[<>]=?]
